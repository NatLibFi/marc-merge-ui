{
  "name": "merge2",
  "version": "0.0.1",
  "description": "Application for merging MARC records",
  "main": "index.js",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "npm run dev",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "npm run build:server && npm run build:frontend",
    "build:frontend": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "build:server": "babel server --out-dir build --source-maps",
    "test": "npm run lint",
    "lint": "eslint .",
    "dev": "npm run watch",
    "watch": "npm run watch:ui & npm run watch:server",
    "watch:ui": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progres",
    "watch:server": "nodemon --watch server --exec babel-node --presets=es2015 server/index.js"
  },
  "keywords": [
    "MARC",
    "merge"
  ],
  "author": "Pasi Tuominen <pasi.tuominen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "cheerio": "^0.22.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.10.1",
    "react": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  }
}
